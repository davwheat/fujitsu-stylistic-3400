;;DELINF;;[Decoder.CopyDrivers]
;;DELINF;;ativrvxx.sys
;;DELINF;;stream.sys

;;DELINF;;[Decoder.CopyCodec]
;;DELINF;;;msyuv.dll

;;DELINF;;[Decoder.UpdateIni]
;;DELINF;;;system.ini, drivers32,,"VIDC.UYVY=msyuv.dll"

;;DELINF;;[Decoder.KnownFiles]
