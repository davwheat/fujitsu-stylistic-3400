[ATITVAudio.AddReg]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,ativsnxx.sys
HKR,,PageOutWhenUnopened,3,01
HKR,,DebugLevel,3,0

;;INFDEL;;[ATITVAudio.CopyDrivers]
;;INFDEL;;stream.sys
;;INFDEL;;;ks.sys
;;INFDEL;;ativsnxx.sys

