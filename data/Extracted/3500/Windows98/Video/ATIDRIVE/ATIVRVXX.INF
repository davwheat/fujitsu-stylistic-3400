;,KSCAPTUR.Registration.NT,MSPCLOCK.Installation
AddReg   =  Decoder.AddReg
;UpdateInis= Decoder.UpdateIni
;;DELINF;;CopyFiles=  Decoder.CopyDrivers
;;DELINF;;DelFiles =  Decoder.DelDrivers

[DecoderPro.Device]
Include  =  ks.inf, ativmvxx.inf
;, kscaptur.inf, ksfilter.inf
Needs=      KS.Registration, DefaultInstall
;,KSCAPTUR.Registration,MSPCLOCK.Installation
AddReg   =  Decoder.AddReg, DecoderPro.AddReg
;UpdateInis= Decoder.UpdateIni
;;DELINF;;CopyFiles=  Decoder.CopyDrivers
;;DELINF;;DelFiles =  Decoder.DelDrivers
;;DELINF;;KnownFiles= Decoder.KnownFiles

[DecoderPro.Device.NT]
Include  =  ks.inf, ativmvxx.inf
;, kscaptur.inf, ksfilter.inf
Needs=      KS.Registration, DefaultInstall
;,KSCAPTUR.Registration.NT,MSPCLOCK.Installation
AddReg   =  Decoder.AddReg, DecoderPro.AddReg
;UpdateInis= Decoder.UpdateIni
