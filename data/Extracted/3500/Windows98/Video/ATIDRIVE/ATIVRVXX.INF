[ATI]
;%Decoder.DeviceDesc%=Decoder.Device,DISPLAY\ATIRTVIDEO
%DecoderPro.DeviceDesc%=DecoderPro.Device,DISPLAY\ATIRTVIDEO

[Decoder.Device]
