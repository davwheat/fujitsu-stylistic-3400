; ativxbxx.INF  -- This file installs ativxbxx.sys
;
;*** Created 6/6/97  (Creation Date)
;

[Version]
CatalogFile=ATII9XAB.CAT
signature="$CHICAGO$"
Class=MEDIA
ClassGUID={4d36e96c-e325-11ce-bfc1-08002be10318}
Provider=%AtiTech%
LayoutFile=LAYOUT.INF, layout1.inf, layout2.inf
DriverVer=02/12/2001

;;DELINF;;[SourceDisksNames]
;;DELINF;;1="ATI WDM Drivers",,,c:\ati\install

;;DELINF;;[SourceDisksFiles]
;;DELINF;;ativxbxx.sys=1

; F i l e   c o p y i n g   s e c t i o n s (where the files go to).

;;DELINF;;[DestinationDirs]
;;DELINF;;ATIXBAR.CopyDrivers=10,system32\drivers  ; eg.  \windows\system32\drivers

[Manufacturer]
%ATI%=ATI

; =================== Generic ==================================

[ATI]
%atixbar.DeviceDesc%=atixbar.Device,DISPLAY\ATIXBAR

[atixbar.Device]
Include  =  ks.inf
;, kscaptur.inf
Needs    =  KS.Registration
;,KSCAPTUR.Registration
AddReg   =  ATIXBAR.addreg
;;DELINF;;CopyFiles=  ATIXBAR.CopyDrivers
;;DELINF;;KnownFiles= ATIXBAR.KnownFiles

;;DELINF;;[ATIXBAR.CopyDrivers]
;;DELINF;;stream.sys
;;DELINF;;ativxbxx.sys

[ATIXBAR.AddReg]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,ativxbxx.sys
HKR,,PageOutWhenUnopened,3,01
HKR,,DebugLevel,3,0

;;DELINF;;[ATIXBAR.KnownFiles]
;;DELINF;;ativxbxx.sys=safe

; =================== atixbar ==================================


[atixbar.Device.Interfaces]
AddInterface={a799a801-a46d-11d0-a18c-00a02401dcd4},"GLOBAL",Crossbar,

[Crossbar]
AddReg=Crossbar.AddReg

[Crossbar.AddReg]
HKR,,CLSID,,{71F96460-78F3-11d0-A18C-00A0C9118956}
HKR,,FriendlyName,,%atixbar.Crossbar%

[Strings]
AtiTech="ATI Technologies Inc."
ATI="ATI"
captureClassName="Sound, video and game controllers"
atixbar.DeviceDesc="ATI WDM AV Crossbar"
atixbar.Crossbar="ATI AV Crossbar"
REG_DWORD=0x00010001
